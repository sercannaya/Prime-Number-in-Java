import java.util.Scanner;
public class FindPrimeNumber {

		public static void main(String[] args) {
			
			int num, i, count = 0;
			Scanner scan = new Scanner(System.in);
			
			System.out.println("Welcome. What do you want to know which number is prime?");
			num = scan.nextInt();
			
				for(i = 2; i < num; i++)
				{
					if (num % i == 0)
					{
						count++;
						break;
					}
				}
				
				System.out.println("Value entered: "+num);
				
				if ( count == 0 )
				{
					System.out.print("Selected number is PRIME.");
				}
				else
				{
					System.out.print("Selected number is NOT PRIME");
				}

		}

	}
